class Node:
    def __init__(self,data):
        self.value=data
        self.next=None
Head=Tail=Node(10)
Tail.next=Node(20)
Tail=Tail.next
Tail.next=Node(30)
Tail=Tail.next
print(Head)
print(Tail)
def print_link_list(Head):
    if Head==None:
        print("list is empty")
        return
    curr=Head
    while curr!=None:
        print(curr.value)
        curr=curr.next
print(print_link_list(Head)) 
