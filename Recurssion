###recurssion of fib
def fib(n):
    if n==1:
        return 0
    if n==2:
        return 1
    return fib(n-1)+fib(n-2)
if __name__=="__main__":
    n=int(input())
    print(fib(n))
#####Factorial
def fact(n):
    if n==1:
        return 1
    return n*fact(n-1)
if __name__=="__main__":
    n=int(input())
    print(fact(n))
