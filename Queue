class Queue:
    def __init__(self):
        self.item=[]
    def push(self,data):
        self.item.append(data)
    def pop(self):
        return self.item.pop(0)
    def size(self):
        return len(self.item)
q=Queue()
q.push(12)
q.push(13)
q.push(14)
q.push(15)
print(q.item)
print(q.size())
print(q.pop())
print(q.item)
